*nat
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]
-A PREROUTING -i wlan0 -p tcp --dport 80 -j REDIRECT --to-port 80
-A PREROUTING -s 192.168.12.0/24 -d 192.168.12.1 -p udp --dport 53 -j REDIRECT --to-port 5353
-A PREROUTING -s 192.168.12.0/24 -d 192.168.12.1 -p tcp --dport 53 -j REDIRECT --to-port 5353
-A POSTROUTING -s 192.168.12.0/24 ! -o wlan0 -j MASQUERADE
COMMIT
